{
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/sitemap-:index.xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "iconodule.fr" }],
      "destination": "https://www.iconodule.fr/:path*",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/heartbeat?token=d9ed2f3e768455f287fa81065ea7557ab6f8fdf006c5c9e66084562613aaa571",
      "schedule": "0 3 1,15 * *"
    }
  ]
}
